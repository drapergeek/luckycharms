<h2>Bookmarks</h2>
<table class='table table-striped'>
  <thead>
    <tr>
      <th>Name</th>
      <th>Url</th>
    </tr>
  </thead>
  <% @bookmarks.each do |bookmark| %>
    <tr>
      <td><%= bookmark.name %></td>
      <td><%= bookmark.url %></td>
      <td><%= link_to 'Delete', admin_bookmark_path(bookmark), method: :delete %></td>
    </tr>
  <% end %>
</table>
